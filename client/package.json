{
  "name": "gatsby-starter-typescript-deluxe",
  "description": "A Gatsby starter with TypeScript, Storybook, Styled Components, Jest, and more.",
  "version": "0.0.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gojutin/gatsby-starter-typescript-deluxe"
  },
  "bugs": {
    "url": "https://github.com/gojutin/gatsby-starter-typescript-deluxe/issues"
  },
  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "lint:watch": "esw -w . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "npm run lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "serve": "gatsby serve",
    "storybook": "start-storybook -p 6006",
    "clean": "gatsby clean",
    "build": "gatsby build",
    "build:storybook": "build-storybook",
    "update": "ncu -u"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "framer-motion": "^2.3.0",
    "gatsby": "^2.24.42",
    "gatsby-image": "^2.4.15",
    "gatsby-plugin-manifest": "^2.4.22",
    "gatsby-plugin-offline": "^3.2.23",
    "gatsby-plugin-react-axe": "^0.4.1",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.26",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-plugin-typescript": "^2.4.18",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-transformer-sharp": "^2.5.12",
    "modern-css-reset": "^1.1.1",
    "netlify-plugin-cypress": "^1.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.1.1",
    "use-media": "^1.4.0"
